# Ticket Master
TICKETMASTER_API_KEY=your_ticketmaster_api_key_here
TICKETMASTER_CLIENT_SECRET=<your Consumer Secret>

# Spotify
SPOTIFY_CLIENT_ID=
SPOTIFY_CLIENT_SECRET=
